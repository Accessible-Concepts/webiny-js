{
  "name": "@webiny/api-dynamic-pages",
  "version": "4.14.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git",
    "directory": "packages/api-files"
  },
  "description": "The API for Webiny Admin's File Manager module.",
  "contributors": [
    "Pavel Denisjuk <pavel@webiny.com>",
    "Sven Al Hamad <sven@webiny.com>",
    "Adrian Smijulj <adrian@webiny.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@webiny/api-page-builder": "^4.14.0",
    "@webiny/graphql": "^4.14.0",
    "node-fetch": "^2.6.1",
    "graphql": "^14.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts\"",
    "watch": "babel src -d dist --source-maps --copy-files --extensions \".ts\" --watch",
    "postbuild": "cp package.json LICENSE README.md dist/ && tsc -p tsconfig.build.json"
  },
  "adio": {
    "ignoreDirs": [
      "__tests__"
    ],
    "ignore": {
      "src": [
        "aws-sdk"
      ]
    }
  }
}
